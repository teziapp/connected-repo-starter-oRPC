{
	"author": "Balkrishna Agarwal",
	"description": "Common Material-UI components for the monorepo",
	"devDependencies": {
		"@connected-repo/typescript-config": "*",
		"@types/react": "^19.2.2"
	},
	"exports": {
		"./components/*": {
			"default": "./dist/components/*.js",
			"import": "./dist/components/*.js",
			"types": "./src/components/*.tsx"
		},
		"./data-display/*": {
			"default": "./dist/data-display/*.js",
			"import": "./dist/data-display/*.js",
			"types": "./src/data-display/*.ts"
		},
		"./feedback/*": {
			"default": "./dist/feedback/*.js",
			"import": "./dist/feedback/*.js",
			"types": "./src/feedback/*.tsx"
		},
		"./form/*": {
			"default": "./dist/form/*.js",
			"import": "./dist/form/*.js",
			"types": "./src/form/*.tsx"
		},
		"./icons/*": {
			"default": "./dist/icons/*.js",
			"import": "./dist/icons/*.js",
			"types": "./src/icons/*.ts"
		},
		"./layout/*": {
			"default": "./dist/layout/*.js",
			"import": "./dist/layout/*.js",
			"types": "./src/layout/*.tsx"
		},
		"./mrt/*": {
			"default": "./dist/mrt/*.js",
			"import": "./dist/mrt/*.js",
			"types": "./src/mrt/*.tsx"
		},
		"./navigation/*": {
			"default": "./dist/navigation/*.js",
			"import": "./dist/navigation/*.js",
			"types": "./src/navigation/*.ts"
		},
		"./rhf-form/*": {
			"default": "./dist/rhf-form/*.js",
			"import": "./dist/rhf-form/*.js",
			"types": "./src/rhf-form/*.tsx"
		},
		"./theme/*": {
			"default": "./dist/theme/*.js",
			"import": "./dist/theme/*.js",
			"types": "./src/theme/*.tsx"
		}
	},
	"license": "AGPL-3.0-only",
	"name": "@connected-repo/ui-mui",
	"peerDependencies": {
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"@hookform/resolvers": "^5.2.2",
		"@mui/icons-material": "^7.3.4",
		"@mui/material": "^7.3.4",
		"material-react-table": "3.2.1",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"react-hook-form": "^7.66.0"
	},
	"private": true,
	"repository": "git@github.com:teziapp/connected-repo-starter-oRPC.git",
	"scripts": {
		"build": "tsc && npx tsc-alias",
		"check-types": "tsc --noEmit",
		"clean": "rm -rf node_modules && rm -rf dist",
		"dev": "concurrently --kill-others \"tsc -w\" \"npx tsc-alias -w\"",
		"format": "biome format . --write",
		"lint": "biome check .",
		"typecheck": "tsc --noEmit"
	},
	"sideEffects": false,
	"type": "module",
	"version": "1.0.0"
}